{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["verification_token", "expires_at"],
  "properties": {
    "verification_token": {
      "type": "string",
      "minLength": 1
    },
    "expires_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false
}
